<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.clone.clone"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="23" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:name="com.parse.clone.StarterApplication"
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@drawable/chat"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.parse.clone.Splash_Screen"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.parse.clone.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.parse.clone.MainActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name="com.parse.clone.UserListActivity" />
        <activity android:name="com.parse.clone.ChatActivity" />
    </application>

</manifest>